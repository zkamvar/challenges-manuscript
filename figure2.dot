digraph G {
	node[shape="box"]
		linelist;
		contacts;
		
	node[shape="box", style = "dashed"]
		WGS;
		
	node[shape="diamond", style="solid"]
		incidence;
		growth_rate;
		phylogeny;
		clustering;
		case_delays;
		paired_delays;
		interactive_graphs;
		
	node[shape="oval"]
		R0;
		serial_interval;
		generation_time;
		transmission_trees;

	node[shape="circle"]
		projections;
		
	node[shape = "none", width = 0, height = 0, label = ""]
		GrR0;
		InRO;
		R0P;
		TT;
		
	node[shape="box", style = "invis"]
		background;

	linelist -> incidence;
	incidence -> growth_rate;

	incidence       -> InR0 [arrowhead = "none"];
        serial_interval -> InR0 [arrowhead = "none"];
	InR0            -> R0;

